{"ast":null,"code":"import plkData from \"../../plk_data.json\";\nimport \"../style.scss\";\nimport LeafletMap from \"../components/LeafletMap.vue\";\nexport default {\n  name: \"PLKView\",\n  data() {\n    return {\n      plkData: plkData,\n      activeTab: \"standings\",\n      // Default active tab\n      showOnlyPLKMarkers: false // Flag to control visibility of only PLK markers\n    };\n  },\n  methods: {\n    showStandings() {\n      this.activeTab = \"standings\";\n      this.showOnlyPLKMarkers = false; // Hide only PLK markers when switching tabs\n    },\n    showResults() {\n      this.activeTab = \"results\";\n      this.showOnlyPLKMarkers = false; // Hide only PLK markers when switching tabs\n    },\n    showMap() {\n      this.activeTab = \"map\";\n      this.showOnlyPLKMarkers = true; // Show only PLK markers\n    }\n  },\n  components: {\n    LeafletMap // Register LeafletMap component\n  }\n};","map":{"version":3,"names":["plkData","LeafletMap","name","data","activeTab","showOnlyPLKMarkers","methods","showStandings","showResults","showMap","components"],"sources":["/home/shymoon/Desktop/WebDevProjects/basket-world/src/views/Plk.vue"],"sourcesContent":["<template>\n  <div>\n    <h1>Season 2023/2024</h1>\n    <label for=\"season\">Choose season:</label>\n    <select id=\"season\">\n      <option value=\"2023-2024\">2023/2024</option>\n    </select>\n\n    <!-- Menu -->\n    <div class=\"menu\">\n      <button @click=\"showStandings\">Standings</button>\n      <button @click=\"showResults\">Results</button>\n      <button @click=\"showMap\">Map</button>\n    </div>\n\n    <!-- Content Area -->\n    <div class=\"content\">\n      <table v-if=\"activeTab === 'standings'\">\n        <!-- Standings Table -->\n        <!-- Your standings table content here -->\n      </table>\n      <div v-else-if=\"activeTab === 'results'\">\n        <!-- Results Content -->\n        <p>Results content goes here.</p>\n      </div>\n      <div v-else-if=\"activeTab === 'map'\">\n        <!-- Map Component -->\n        <p v-if=\"showOnlyPLKMarkers\">Map component goes here.</p>\n        <LeafletMap v-else />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport plkData from \"../../plk_data.json\";\nimport \"../style.scss\";\nimport LeafletMap from \"../components/LeafletMap.vue\";\n\nexport default {\n  name: \"PLKView\",\n  data() {\n    return {\n      plkData: plkData,\n      activeTab: \"standings\", // Default active tab\n      showOnlyPLKMarkers: false, // Flag to control visibility of only PLK markers\n    };\n  },\n  methods: {\n    showStandings() {\n      this.activeTab = \"standings\";\n      this.showOnlyPLKMarkers = false; // Hide only PLK markers when switching tabs\n    },\n    showResults() {\n      this.activeTab = \"results\";\n      this.showOnlyPLKMarkers = false; // Hide only PLK markers when switching tabs\n    },\n    showMap() {\n      this.activeTab = \"map\";\n      this.showOnlyPLKMarkers = true; // Show only PLK markers\n    },\n  },\n  components: {\n    LeafletMap, // Register LeafletMap component\n  },\n};\n</script>\n\n<style scoped></style>\n"],"mappings":"AAmCA,OAAOA,OAAM,MAAO,qBAAqB;AACzC,OAAO,eAAe;AACtB,OAAOC,UAAS,MAAO,8BAA8B;AAErD,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLH,OAAO,EAAEA,OAAO;MAChBI,SAAS,EAAE,WAAW;MAAE;MACxBC,kBAAkB,EAAE,KAAK,CAAE;IAC7B,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd,IAAI,CAACH,SAAQ,GAAI,WAAW;MAC5B,IAAI,CAACC,kBAAiB,GAAI,KAAK,EAAE;IACnC,CAAC;IACDG,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACJ,SAAQ,GAAI,SAAS;MAC1B,IAAI,CAACC,kBAAiB,GAAI,KAAK,EAAE;IACnC,CAAC;IACDI,OAAOA,CAAA,EAAG;MACR,IAAI,CAACL,SAAQ,GAAI,KAAK;MACtB,IAAI,CAACC,kBAAiB,GAAI,IAAI,EAAE;IAClC;EACF,CAAC;EACDK,UAAU,EAAE;IACVT,UAAU,CAAE;EACd;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}