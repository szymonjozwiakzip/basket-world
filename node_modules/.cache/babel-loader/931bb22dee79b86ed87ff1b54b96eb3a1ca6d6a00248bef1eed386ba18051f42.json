for=\"(team, index) in teams\" :key=\"index\">\n            <td>{{ team.position }}</td>\n            <td>{{ team.name }}</td>\n            <td>{{ team.points }}</td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport cheerio from \"cheerio\";\n\nexport default {\n  name: \"PlkView\",\n  data() {\n    return {\n      isLoading: false,\n      error: null,\n      teams: [],\n    };\n  },\n  mounted() {\n    this.fetchData();\n  },\n  methods: {\n    fetchData() {\n      this.isLoading = true;\n      axios\n        .get(\n          \"https://www.flashscore.pl/tabela/Aw8ey5Pa/IN29bO3h/#/IN29bO3h/table/overall\"\n        )\n        .then((response) => {\n          const $ = cheerio.load(response.data);\n          const teams = [];\n          $(\".table__body tr\").each((index, element) => {\n            const position = $(element)\n              .find(\".table__cell--rank\")\n              .text()\n              .trim();\n            const name = $(element)\n              .find(\".team__cell .team__name\")\n              .text()\n              .trim();\n            const points = $(element)\n              .find(\".table__cell--points\")\n              .text()\n              .trim();\n            teams.push({ position, name, points });\n          });\n          this.teams = teams;\n          this.isLoading = false;\n        })\n        .catch((error) => {\n          console.error(\"Błąd pobierania danych:\", error);\n          this.error = \"Wystąpił błąd podczas pobierania danych.\";\n          this.isLoading = false;\n        });\n    },\n  },\n};\n</script>\n\n<style scoped>\n.error {\n  color: red;\n}\n</style>\n"],"mappings":";AA0BA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,SAAS;AAE7B,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,KAAK;MAChBC,KAAK,EAAE,IAAI;MACXC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,CAAC;EAClB,CAAC;